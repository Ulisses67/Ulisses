10 PRINT "CALCULO NOTA MINIMA PARA VF:"
20 INPUT "INSIRA SUA NOTA DE VE:"; VE
30 IF VE<0 OR VE>10 THEN PRINT "NOTA INCORRETA. DIGITE NOVAMENTE." : GOTO 20 
40 INPUT "DIGITE SUA NOTA DE VC:"; VC
50 IF VC<0 OR VC>10 THEN PRINT "NOTA INCORRETA. DIGITE NOVAMENTE." : GOTO 40
60 LET GIRO=(20-VC-VE)/4
70 IF GIRO<4 THEN VF=4
80 IF GIRO>4 THEN VF=GIRO
90 PRINT "EIS A NOTA QUE DEVES TIRAR NA VF:"; VF
100 PRINT "BONS ESTUDOS"
